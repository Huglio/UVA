#include <bits/stdc++.h>

using namespace std;
typedef pair<int,int>ii;
typedef vector<int>vi;
#define inf 0x3f
int ponte = 0;
vi AdjList[51];
int h[51],b[51];
bool  vis[51];

void dfs(int u, int d, int pai)
{
	vis[u] = 1; b[u] = h[u] = d;
	
	for (int i = 0; i < (int)AdjList[u].size(); i++)
	{
		int a = AdjList[u][i];
		if (!vis[a]){
			dfs(a, d+1, u);
			b[u] = min(b[a], b[u]);
			if (b[a] > h[u]) ponte++;
			
		}else if (a != pai){
			b[u] = min(b[u], h[a]);
		}			
	}
}

int main()
{
	//freopen("in","r", stdin);
	int lig1,lig2,V,A;
	
	while(scanf("%d%d",&V,&A)!=EOF)
	{
		ponte = 0;
		for (int i = 0; i < A; i++)
		{
			scanf("%d%d",&lig1,&lig2);
			AdjList[lig1].push_back(lig2);
			AdjList[lig2].push_back(lig1);
		}	
		memset(vis,false,sizeof vis);
		memset(h,inf,sizeof h);	
		memset(b,inf,sizeof b);	
		
		for (int i = 1; i <= V; i++)
		{
			if (!vis[i])
			{
				dfs(i, 0, i);
			}		
		}	
		printf("%d\n",ponte);
		
		for (int i = 0; i < 51; i++)
		{
			AdjList[i].clear();
		}
		
	}

}
